import request from '@/utils/request_civil'

export function addCiviWorkerInfo(data) {
  return request({
    url: '/civi/addCiviWorkerInfo',
    method: 'post',
    data,
  })
}

export function selectList(data) {
  return request({
    url: '/civi/selectList',
    method: 'post',
    data,
  })
}

export function info(data) {
  return request({
    url: '/civi/info?id=' + data,
    method: 'post',
  })
}


