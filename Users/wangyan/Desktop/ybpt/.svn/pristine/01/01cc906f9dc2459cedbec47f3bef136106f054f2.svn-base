}


.bottom {
  padding: 25px 0;
  color: #595959;
  text-align: left;
  border-top: 1px solid $base-border-color;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  text-align: center;
  z-index: 10;
}