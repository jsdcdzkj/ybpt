  // .el-upload {
  //   display: block;

  //   .el-upload-dragger {
  //     width: 100% !important;
  //   }
  // }

  .boxtable {
    margin-bottom: 20px;
    border: 1px dashed #dfdfdf;
    border-radius: 5px;
    background: #f8f8f8;
    padding: 10px 20px 20px;

    p {
      font-size: 15px;
      font-weight: bold;

      span {
        font-size: 12px;
        color: #ff0000;
        font-weight: 100;
      }
    }

    .btn-box {
      margin-top: 10px;
    }
  }

  .boxtable:last-child {
