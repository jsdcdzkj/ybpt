import request from '@/utils/request'

export function selectList(data) {
  return request({
    url: '/unfixedMechanism/selectList',
    method: 'post',
    data,
  })
}

export function addUnfixedMechanism(data) {
  return request({
    url: '/unfixedMechanism/addUnfixedMechanism',
    method: 'post',
    data,
  })
}

export function updateUnfixedMechanism(data) {
  return request({
    url: '/unfixedMechanism/updateUnfixedMechanism',
    method: 'post',
    data,
  })
}

export function delUnfixedMechanism(data) {
  return request({
    url: '/unfixedMechanism/delUnfixedMechanism',
    method: 'post',
    data,
  })
}


export function info(data) {
  return request({
    url: '/unfixedMechanism/info?id='+data,
    method: 'post'
  })
}

