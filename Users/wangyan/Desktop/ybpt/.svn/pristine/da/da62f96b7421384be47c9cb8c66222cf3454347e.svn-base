import request from '@/utils/request'

export function addAu(data) {
  return request({
    url: '/administrative_unit/addAu',
    method: 'post',
    data,
  })
}

export function selectList(data) {
  return request({
    url: '/administrative_unit/selectList',
    method: 'post',
    data,
  })
}

export function selectListAll() {
  return request({
    url: '/administrative_unit/selectListAll',
    method: 'post',
  })
}




export function delAu(data) {
  return request({
    url: '/administrative_unit/delAu',
    method: 'post',
    data,
  })
}

export function updateAu(data) {
  return request({
    url: '/administrative_unit/updateAu',
    method: 'post',
    data,
  })
}

export function info(data) {
  return request({
    url: '/administrative_unit/info?id='+data,
    method: 'post',
  })
}



