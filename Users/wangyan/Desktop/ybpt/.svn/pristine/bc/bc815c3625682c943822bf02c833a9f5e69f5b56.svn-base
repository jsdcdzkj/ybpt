import request from '@/utils/request'

export function selectByIdCard(data) {
  return request({
    url: '/opspRegd/selectByIdCard',
    method: 'post',
    params: data,
  })
}

export function selectCompanyByIdCard(data) {
  return request({
    url: '/opspRegd/selectCompanyByIdCard',
    method: 'post',
    data,
  })
}