    public String getEmployingInfoListCheckedByOrg(String medicalInsuranceNum) {
        String sql = "\tSELECT  DISTINCT ei.EMP_NO,ei.EMP_NAME FROM PERSON_SUBSCRIBE_RECORD psr \n" +
                "\t LEFT JOIN CIVILWORKER_INFO ci ON psr.CIVILWORKER_ID = ci.CERTNO \n" +
                "\t INNER JOIN EMPLOYING_INFO ei ON ei.EMP_NO = ci.EMP_CODE WHERE   psr.IS_DEL = '0' \n"+

                "\t AND psr.ORG_ID = '" +medicalInsuranceNum+ "' \n";
        return sql;
    }

    public String getDepartmentListByEmpCode(String empCode) {
        String sql = "SELECT\n" +
                "\tDI.EMP_CODE,\n" +
                "\tDI.DEPT_NO,\n" +
                "\tDI.DEPT_NAME,\n" +
                "\tDI.ID,\n" +
                "\tDI.IS_DEL\n" +
                "FROM\n" +
                "\tDEPT_INFO DI\n" +
                "\tINNER JOIN EMPLOYING_INFO EI ON EI.EMP_NO = DI.EMP_CODE\n" +
                "\tWHERE EI.EMP_NO = '"+ empCode + "'" +
                "\tAND DI.IS_DEL = '0'";
        return sql;
    }
