import request from '@/utils/request'

export function getList(data) {
  return request({
    url: '/sysuser/getList',
    method: 'post',
    params: data,
  })
}

export function saveUser(data) {
  return request({
    url: '/sysuser/saveUser',
    method: 'post',
    data,
  })
}

