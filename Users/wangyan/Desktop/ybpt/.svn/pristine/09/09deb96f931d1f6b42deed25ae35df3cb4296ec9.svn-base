<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jsdc.ybpt.mapper.formula.ManageLossOtherFeeMapper">

    <select id="selectDataList" resultType="com.jsdc.ybpt.formula.domain.entity.ManageLossOtherFee">
        select
            t1.ID,
            t1.PRICE_CALCULATE_ID,
            t1.MANAG_FEE,
            t1.LOSS_OTHER_FEE,
            t1.IS_DEL,
            t1.CREATETIME,
            t1.CREATEUSER,
            t1.UPDATETIME,
            t1.UPDATEUSER,
            t1.IS_SUBTOTAL

        from FORMULA_MANAGE_LOSS_OTHER_FEE t1
        <where>
      
            <if test="id != null and id != ''">
                and t1.ID = #{id}
            </if>
            <if test="priceCalculateId != null and priceCalculateId != ''">
                and t1.PRICE_CALCULATE_ID = #{priceCalculateId}
            </if>
            <if test="managFee != null">
                and t1.MANAG_FEE = #{managFee}
            </if>
            <if test="lossOtherFee != null">
                and t1.LOSS_OTHER_FEE = #{lossOtherFee}
            </if>
            <if test="isDel != null and isDel != ''">
                and t1.IS_DEL = #{isDel}
            </if>
            <if test="createtime != null">
                and t1.CREATETIME = #{createtime}
            </if>
            <if test="createuser != null and createuser != ''">
                and t1.CREATEUSER = #{createuser}
            </if>
            <if test="updatetime != null">
                and t1.UPDATETIME = #{updatetime}
            </if>
            <if test="updateuser != null and updateuser != ''">
                and t1.UPDATEUSER = #{updateuser}
            </if>
            <if test="isSubtotal != null and isSubtotal != ''">
                and t1.IS_SUBTOTAL = #{isSubtotal}
            </if>
        </where>
    </select>
    
    
    <select id="pageByDTO" resultType="com.jsdc.ybpt.formula.domain.entity.ManageLossOtherFee"
        parameterType="com.jsdc.ybpt.formula.domain.dto.ManageLossOtherFeeDTO">
        select
            t1.ID,
            t1.PRICE_CALCULATE_ID,
            t1.MANAG_FEE,
            t1.LOSS_OTHER_FEE,
            t1.IS_DEL,
            t1.CREATETIME,
            t1.CREATEUSER,
            t1.UPDATETIME,
            t1.UPDATEUSER,
            t1.IS_SUBTOTAL

        from FORMULA_MANAGE_LOSS_OTHER_FEE t1
        <where>
        </where>
        
    </select>
    
    
    
    <select id="getVOById" resultType="com.jsdc.ybpt.formula.domain.entity.ManageLossOtherFee"
        parameterType="int">
        select
            t1.ID,
            t1.PRICE_CALCULATE_ID,
            t1.MANAG_FEE,
            t1.LOSS_OTHER_FEE,
            t1.IS_DEL,
            t1.CREATETIME,
            t1.CREATEUSER,
            t1.UPDATETIME,
            t1.UPDATEUSER,
            t1.IS_SUBTOTAL

        from FORMULA_MANAGE_LOSS_OTHER_FEE t1
        <where>

            and t1.ID = #{id}
            
       
        </where>
        
    </select>
    
    
</mapper>


